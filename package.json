    "react-native-uuid": "^1.4.9",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "realm": "^1.8.3",
    "stream": "^0.0.2",
    "text-encoding": "^0.6.4",
    "urlencode": "^1.1.0"
  },
