{
  "name": "dnaebook",
  "version": "0.3.8",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean-apk": "cd android && gradlew clean",
    "install-apk": "adb install -r ./android/app/build/outputs/apk/app-debug.apk",
    "debug-apk": "cd android && gradlew assembleDebug && npm run install-apk",
    "release-apk": "cd android && gradlew assembleRelease && adb install -r ./app/build/outputs/apk/app-release.apk",
    "bind": "adb reverse tcp:8081 tcp:8081",
    "menu": "adb shell input keyevent 82",
    "test": "jest"
  },
  "dependencies": {
    "antd-mobile": "^1.2.0",
    "axios": "^0.16.1",
    "camelcase": "^4.0.0",
    "cheerio-without-node-native": "^0.20.2",
    "iconv-lite": "^0.4.16",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "node-uuid": "^1.4.8",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.0",
    "react-native-drawer-layout": "^1.3.1",
    "react-native-storage": "^0.1.5",
    "react-native-uuid": "^1.4.9",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "realm": "^1.3.1",
    "text-encoding": "^0.6.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "1.9.1",
    "eslint": "3.15.0",
    "eslint-config-standard": "7.0.0",
    "eslint-config-standard-jsx": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.1.1",
    "jest": "18.1.0",
    "mobx-logger": "^0.4.0",
    "react-test-renderer": "~15.4.0",
    "standard": "*"
  },
  "jest": {
    "preset": "react-native"
  }
}
